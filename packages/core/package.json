{
  "name": "@walkthrough/core",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/walkthrough.cjs",
  "module": "./dist/walkthrough.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/walkthrough.cjs",
      "import": "./dist/walkthrough.js"
    },
    "./dist/walkthrough.css": {
      "require": "./dist/walkthrough.css",
      "import": "./dist/walkthrough.css",
      "default": "./dist/walkthrough.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.14.1",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.1",
    "vite-bundle-analyzer": "^0.19.0",
    "vite-plugin-dts": "^4.2.1"
  },
  "author": "Adeyanju Adeyemi",
  "keywords": [
    "walkthrough",
    "walkthrough.js",
    "tour",
    "guide",
    "product tour",
    "product walkthrough",
    "product guide",
    "product tutorial",
    "product demo"
  ]
}
